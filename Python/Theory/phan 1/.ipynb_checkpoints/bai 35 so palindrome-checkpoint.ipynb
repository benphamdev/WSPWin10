    "def palindrome (n) :\n",
    "    tmp = n\n",
    "    rev = 0\n",
    "    while n != 0 :\n",
    "        rev = rev * 10 + n % 10\n",
    "        n //= 10\n",
    "    return rev == tmp\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    n = int(input())\n",
    "    print(palindrome(n))"
   ]
  }
