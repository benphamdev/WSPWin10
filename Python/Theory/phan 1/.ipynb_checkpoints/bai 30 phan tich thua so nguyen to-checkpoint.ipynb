    "from math import *\n",
    "\n",
    "def pt (n) :\n",
    "    for i in range (2, isqrt(n) + 1) :\n",
    "        if n % i == 0 :\n",
    "            while n % i == 0 :\n",
    "                print (i, end = ' ')\n",
    "                n //= i\n",
    "    if n != 1 : print (n)\n",
    "        \n",
    "if __name__ == '__main__' :\n",
    "    n = int(input())\n",
    "    pt (n)"
   ]
