    "# to hop \n",
    "\n",
    "from math import *\n",
    "\n",
    "def tohop(n, k) :\n",
    "    return factorial(n) // (factorial(k) * factorial(n - k))\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    print(comb(10, 2), tohop(10, 2))\n",
    "\n",
    "# nC2 = (n - 1) * n // 2"
   ]
