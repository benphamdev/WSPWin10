   "source": [
    "n = int(input())\n",
    "if n % 2 == 0 : \n",
    "    print(n // 2)\n",
    "    for i in range(n // 2) : \n",
    "        print(2, end = ' ')\n",
    "elif n >= 3 :\n",
    "    print(1 + (n - 3) // 2)\n",
    "    for _ in range((n - 3) // 2) :\n",
    "        print(2, end = ' ')\n",
    "    print(3, end = ' ') \n",
    "else : \n",
    "    print(-1)"
   ]
