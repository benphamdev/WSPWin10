    "def lp(n) :\n",
    "    while n :\n",
    "        r = n % 10\n",
    "        if (r != 0 and r != 6 and r != 8) : return False\n",
    "        n //= 10\n",
    "    return True\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    n = int(input())\n",
    "    print(1 if lp(n) else 0)"
   ]
  }
