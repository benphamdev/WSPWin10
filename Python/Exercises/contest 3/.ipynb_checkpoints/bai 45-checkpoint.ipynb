    "def check(n) :\n",
    "    for i in range(1, n // 111 + 1) :\n",
    "        if (n - 111 * i) % 11 == 0 :\n",
    "            return True\n",
    "    return False\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    n = int(input())\n",
    "    print(\"YES\" if check(n) else \"NO\")"
   ]
  }
