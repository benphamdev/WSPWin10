    "from math import *\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    n = int(input())\n",
    "    sum = 1\n",
    "    for i in range (2, isqrt(n) + 1) :\n",
    "        if n % i == 0 :\n",
    "            sum += i\n",
    "            if n // i != i :\n",
    "                sum += n // i\n",
    "    sum += n\n",
    "    print(sum)"
