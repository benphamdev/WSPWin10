    "r, c = map (int, input().split())\n",
    "ans = 0\n",
    "if r % 2 == 0 : \n",
    "    ans = c * (r // 2)\n",
    "else : \n",
    "    ans = c * (r - 1) // 2\n",
    "    ans += c // 2\n",
    "print(ans)"
   ]
