    "def fibo(n) :\n",
    "    if n == 1 : return 0\n",
    "    if n == 2 : return 1\n",
    "    return fibo(n - 1) + fibo(n - 2)\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    n = int(input())\n",
    "    print(fibo(n))"
   ]
  }
