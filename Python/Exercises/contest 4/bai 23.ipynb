   "source": [
    "n = int(input())\n",
    "a = list(map(int, input().split()))\n",
    "\n",
    "def check(t) :\n",
    "    if (t == n) :\n",
    "        return True\n",
    "    if(a[t] % 2 == 1) :\n",
    "        return False\n",
    "    return check(t + 1)\n",
    "\n",
    "print(\"YES\" if check(0) else \"NO\")"
   ]
