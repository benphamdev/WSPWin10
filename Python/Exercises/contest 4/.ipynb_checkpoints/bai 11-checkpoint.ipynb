    "def convertbi(n):\n",
    "    if n == 0 : \n",
    "        return 0\n",
    "    convertbi(n // 2)\n",
    "    print(n % 2, end = '')\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    n = int(input())\n",
    "    if n == 0 : \n",
    "        print(0)\n",
    "    else :\n",
    "        convertbi(n)"
