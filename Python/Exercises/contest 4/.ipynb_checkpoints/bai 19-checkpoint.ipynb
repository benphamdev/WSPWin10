    "def check(n) :\n",
    "    if n <= 9 :\n",
    "        return n % 2 == 0\n",
    "    if n % 10 % 2 == 1 : \n",
    "        return False\n",
    "    return check(n // 10)\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    n = int(input())\n",
    "    print(\"YES\" if check(n) else \"NO\")"
   ]
  }
