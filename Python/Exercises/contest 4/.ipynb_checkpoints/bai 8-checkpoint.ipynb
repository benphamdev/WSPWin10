    "def gcd(a, b) :\n",
    "    return a if b == 0 else gcd(b, a % b)\n",
    "\n",
    "def lcm(a, b) :\n",
    "    return a // gcd(a, b) * b\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    a, b = map(int, input().split())\n",
    "    print(gcd(a, b), lcm(a, b))"
   ]
  }
