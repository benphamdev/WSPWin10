    "def nCk(n, k) :\n",
    "    if k == 0 or k == n : return 1\n",
    "    return nCk(n - 1, k - 1) + nCk(n - 1, k)\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    n, k = map(int, input().split())\n",
    "    print(nCk(n, k))"
   ]
  }
