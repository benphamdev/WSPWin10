    "def numDig(n):\n",
    "    if n <= 9 : return 1\n",
    "    return 1 + numDig(n // 10)\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    n = int(input())\n",
    "    print(numDig(n))"
   ]
