   "source": [
    "mod = (int)(1e9 + 7)\n",
    "n = int(input())\n",
    "a = list(map(int, input().split()))\n",
    "sum , mul = 0, 1\n",
    "for i in a :\n",
    "    sum += i\n",
    "    sum %= mod\n",
    "    mul *= i\n",
    "    mul %= mod\n",
    "print(sum % mod, mul % mod, sep = '\\n')"
   ]
  }
