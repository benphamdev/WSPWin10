    "def check(a, n) :\n",
    "    for i in range(0, n // 2) :\n",
    "        if(a[i] != a[n - i - 1]) : return False\n",
    "    return True\n",
    "\n",
    "if __name__ == '__main__' :\n",
    "    n = int(input())\n",
    "    a = list(map(int, input().split()))\n",
    "    print(\"YES\" if check(a, n) else \"NO\")"
   ]
  }
