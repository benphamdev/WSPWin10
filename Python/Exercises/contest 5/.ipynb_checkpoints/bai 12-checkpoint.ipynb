   "source": [
    "n = int(input())\n",
    "a = list(map(int, input().split()))\n",
    "\n",
    "minval, maxval = 10 ** 7, -10 ** 7\n",
    "idx, idx2 = 0, 0\n",
    "for i in range(n):\n",
    "    if a[i] <= minval :\n",
    "        minval = a[i]\n",
    "        idx = i\n",
    "    if a[i] > maxval :\n",
    "        maxval = a[i]\n",
    "        idx2 = i\n",
    "print(idx, idx2)"
   ]
  }
