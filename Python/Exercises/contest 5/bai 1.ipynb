    "n = int(input())\n",
    "a = list(map(int,input().split()))\n",
    "\n",
    "even, odd, sumEven, sumOdd = 0, 0, 0, 0\n",
    "\n",
    "for x in a :\n",
    "    if x % 2 == 0 : \n",
    "        even += 1\n",
    "        sumEven += x\n",
    "    else :\n",
    "        odd += 1\n",
    "        sumOdd += x\n",
    "print(even, odd, sumEven, sumOdd, sep = '\\n')"
   ]
  }
