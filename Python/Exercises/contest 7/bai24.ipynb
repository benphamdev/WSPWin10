    "n, k = map(int, input().split())\n",
    "a = list(map(int, input().split()))\n",
    "a[1:] = sorted(a[1:], reverse = True)\n",
    "sum = a[0]\n",
    "for i in range(1, n) :\n",
    "    if(i <= k) :\n",
    "        sum += a[i]\n",
    "    else : \n",
    "        sum -= a[i]\n",
    "print(sum)"
   ]
