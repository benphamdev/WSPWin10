    "b = list(map(int, input().split()))\n",
    "\n",
    "i, j = 0, 0\n",
    "while i < n and j < m :\n",
    "    if(a[i] <= b[j]) :\n",
    "        i += 1\n",
    "    else :\n",
    "        i += 1\n",
    "        j += 1\n",
    "print(j)"
