   "source": [
    "n, l = map(int, input().split())\n",
    "a = list(map(int, input().split()))\n",
    "\n",
    "a.sort()\n",
    "ans = max(a[0] - 0, l - a[n - 1])\n",
    "for i in range(n - 1) :\n",
    "    tmp = (a[i + 1] - a[i]) / 2\n",
    "    if(tmp > ans) :\n",
    "        ans = tmp\n",
    "\n",
    "print('{:.10f}'. format(ans))"
   ]
  }
