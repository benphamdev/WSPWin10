   "source": [
    "s = input()\n",
    "cnt, cnt1, cnt2 = 0, 0, 0\n",
    "for x in s : \n",
    "    if x.isalpha() : cnt += 1\n",
    "    elif x.isdigit() : cnt1 += 1\n",
    "    else : cnt2 += 1\n",
    "print(cnt, cnt1, cnt2)"
   ]
  }
