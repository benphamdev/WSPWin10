    "s = input()\n",
    "cnt = 0\n",
    "def check() : \n",
    "    global s\n",
    "    l, r = 0, len(s) - 1\n",
    "    while(l <= r) : \n",
    "        if(s[l] != s[r]) : return False\n",
    "        l += 1\n",
    "        r -= 1\n",
    "    return True\n",
    "if __name__ == '__main__' : \n",
    "    print(('YES' if check() else \"NO\"))"
