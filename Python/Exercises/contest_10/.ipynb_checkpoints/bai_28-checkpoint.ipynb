    "if __name__ == '__main__' :\n",
    "    s = input()\n",
    "    s += '#'\n",
    "    ans, tmp = 0, 0\n",
    "    for i in range(len(s)) :\n",
    "        if(s[i].isdigit()) :\n",
    "            tmp = tmp * 10 + (ord(s[i]) - ord('0'))\n",
    "        else :\n",
    "            ans = max(tmp, ans)\n",
    "            tmp = 0\n",
    "    print(ans)"
   ]
